{
  "name": "@session-harbor/cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "CLI for Session Harbor tools",
  "bin": {
    "session-harbor": "dist/cli.cjs"
  },
  "scripts": {
    "start": "tsx src/cli.ts",
    "build": "esbuild src/cli.ts --bundle --platform=node --format=cjs --target=node18 --banner:js='#!/usr/bin/env node' --outfile=dist/cli.cjs",
    "prepack": "npm run build"
  },
  "files": ["dist/cli.cjs"],
  "devDependencies": {
    "esbuild": "^0.25.0",
    "tsx": "^4.7.0"
  },
  "engines": {
    "node": ">=18"
  }
}
